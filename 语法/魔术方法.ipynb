   "source": [
    "# @这个符号的设定是用于矩阵乘法\n",
    "\n",
    "\n",
    "class Point:\n",
    "    def __init__(self, x, y):\n",
    "        self.x = x  # x坐标\n",
    "        self.y = y  # y坐标\n",
    "\n",
    "    def __matmul__(self, other):\n",
    "        x = self.x - other.x\n",
    "        y = self.y - other.y\n",
    "        return x + y\n",
    "\n",
    "\n",
    "a = Point(1, 1)\n",
    "b = Point(4, 5)\n",
    "\n",
    "print(a @ b)\n",
